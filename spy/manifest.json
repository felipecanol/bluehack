{
  "manifest_version": 2,
  "name": "Spy",
  "description": "Replaces words with emojis.",
  "homepage_url": "https://github.com/felipecanol/bluehack",
  "version": "1.0",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./spy.js"]
    }
  ]
}
